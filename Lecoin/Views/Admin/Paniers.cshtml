
@{
    ViewBag.Title = "Paniers";
    Layout = "~/Views/Shared/_LayoutBlanck.cshtml";
}
@model IEnumerable<Lecoin.Models.Panier>
<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-sm-4 one-commande">
                <a href="~/Admin/DeletePaniers/@item.IdPanier" class="btn btn-danger"><i class="fa fa-trash fa-2x"></i></a>
                <p>idArticle @item.IdArticle</p>
            </div>
        }

    </div>
</div>

